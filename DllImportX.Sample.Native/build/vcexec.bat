@ECHO OFF
SETLOCAL

SET ARGS=%*
SET TARGET_ARCH=%1

IF NOT DEFINED __DLLIMPORTX_VCVARSALL__ echo VCVARSALL SCRIPT NOT DEFINED & GOTO :EOF 
IF NOT DEFINED ARGS echo NO ARGS PROVIDED & GOTO :EOF 

CALL SET COMMAND=%%ARGS:*%1=%%

IF NOT DEFINED COMMAND echo NO COMMAND PROVIDED & GOTO :EOF 

CALL "%__DLLIMPORTX_VCVARSALL__%" %TARGET_ARCH% 1> NUL

CALL %comspec% /c %COMMAND%